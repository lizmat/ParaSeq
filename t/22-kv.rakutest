use Test;
use ParaSeq;

plan 2;

my constant $elems = 200000;
my constant $list  = (^$elems).pick(*).List;
my constant $batch = 16;
my constant $kv    = $list.kv.List;

for 1, ParaSeq.default-degree {
    is-deeply $list.&hyperize($batch, $_).kv.List, $kv,
      ".kv with degree = $_";
}

