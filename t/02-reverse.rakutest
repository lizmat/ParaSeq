use Test;
use ParaSeq;

plan 4;

my constant @list = (^200000).List;
my constant @tsil = @list.reverse;

for 1, ParaSeq.default-degree {
    my $seq := @list.&hyperize(2000, $_).reverse;
    isa-ok $seq, $_ == 1 ?? Seq !! ParaSeq;
    is-deeply $seq.List, @tsil, "reverse with degree = $_";
}

