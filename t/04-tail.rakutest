use Test;
use ParaSeq;

plan 6;

my constant @list  = (^200000).List;
my constant $tail  = @list.tail;
my constant $tail3 = @list.tail(3).List;

for 1, ParaSeq.default-degree {
    is-deeply @list.&hyperize(2000, $_).tail, $tail,
      "tail with degree $_";

    my $seq := @list.&hyperize(2000, $_).tail(3);
    todo "huh";
    isa-ok $seq, $_ == 1 ?? Seq !! ParaSeq;
    is-deeply $seq.List, $tail3, "tail(3) with degree = $_";
}

