use Test;
use ParaSeq;

plan 2;

my constant $elems = 200000;
my constant $list  = (^$elems).pick(*).List;
my constant $batch = 16;
my constant $pairs = $list.pairs.List;

for 1, ParaSeq.default-degree {
    is-deeply $list.&hyperize($batch, $_).pairs.List, $pairs,
      ".pairs with degree = $_";
}

